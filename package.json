{
  "name": "twitter-coding-resource-bot",
  "version": "1.0.0",
  "description": "a twitter bot for coding resources",
  "main": "bot.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run prod:buildbot; node ./server-dist/bot.js",
